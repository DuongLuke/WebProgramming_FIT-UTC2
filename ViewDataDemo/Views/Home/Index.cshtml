
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h2>THIS IS THE INDEX PAGE WITH FIRST ACTION METHOD</h2>
        <h2>@ViewData["Message"]</h2>
        <h2>@ViewData["CurrentTime"]</h2>
    </div>
    <div>
        <ul>
            @{
            foreach (string i in (string[])ViewData["FruitArray"]){
                <li>@i</li>
            }

            }
        </ul>
        <hr />
        <ul>
            @{
     foreach (string i in (List<string>)ViewData["SportList"]){
                <li>@i</li>
     }

            }
        </ul>
        <hr />
        @{
        var a = (WebApplicationMVC.Models.Employee)ViewData["Employee"];
            <p>@a.EmpId</p>
            <p>@a.EmpName</p>
            <p>@a.EmpDept</p>
        }

    </div>
</body>
</html>
